{
  "extends": "./node_modules/electron-webpack/tsconfig-base.json",
  "compilerOptions": {
    "jsx": "react",
    "target": "es2019",
    "esModuleInterop": true,
    "skipLibCheck": true
  },
  "exclude": [
    "node_modules",
    "src/**/*.test.ts",
    "src/**/*.test.tsx"
  ],
  "include": [
    "global.d.ts",
    "src/common/store/types.d.ts",
    "src/**/*.ts",
    "src/**/*.tsx"
  ]
}
